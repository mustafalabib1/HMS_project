@model ClinicViewModel
@{
    ViewData["Title"] = "View Clinic";
}
<h1>Clinic Details</h1>
<br />
<br />


<div class="mb-3">
    <label asp-for="Name" class="form-label"></label>
    <input asp-for="Name" class="form-control" readonly />
    <span asp-validation-for="Name" class="text-danger"></span>
</div>
<div class="mb-3">
    <label asp-for="Specialization" class="form-label"></label>
    <input asp-for="Specialization" class="form-control" readonly />
    <span asp-validation-for="Specialization" class="text-danger"></span>
</div>
<div class="mb-3">
    <label asp-for="Phone" class="form-label"></label>
    <input asp-for="Phone" class="form-control" readonly />
    <span asp-validation-for="Phone" class="text-danger"></span>
</div>
<div class="mb-3">
    <label asp-for="Price" class="form-label"></label>
    <input asp-for="Price" class="form-control" readonly />
    <span asp-validation-for="Price" class="text-danger"></span>
</div>

<a asp-controller="Clinic" asp-action="Index" class="btn btn-primary mt-2">Back To List</a>